{
    "name": "vscode-wrap-console-log",
    "displayName": "Wrap Console Log",
    "description": "Wrap to console.log by word or selection",
    "repository": {
        "type": "git",
        "url": "https://github.com/midnightsyntax/vscode-wrap-console-log"
    },
    "version": "1.3.3",
    "publisher": "midnightsyntax",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Formatters"
    ],
    "keywords": [
        "wrap",
        "console",
        "log"
    ],
    "activationEvents": [
        "onCommand:midnight.console.log.wrap.Inline",
        "onCommand:midnight.console.log.wrap.InlinePrefix",
        "onCommand:midnight.console.log.wrap.InlinePrefixInput",
        "onCommand:midnight.console.log.wrap.Up",
        "onCommand:midnight.console.log.wrap.UpPrefix",
        "onCommand:midnight.console.log.wrap.UpPrefixInput",
        "onCommand:midnight.console.log.wrap.Down",
        "onCommand:midnight.console.log.wrap.DownPrefix",
        "onCommand:midnight.console.log.wrap.DownPrefixInput"
    ],
    "main": "out/extension.js",
    "contributes": {
        "configuration": {
            "title": "Wrap Console Log",
            "type": "object",
            "properties": {
                "wrap-console-log.alwaysInputBoxOnPrefix": {
                    "type": "boolean",
                    "title": "Always show input box when wrapping with a prefix",
                    "description": "If true an input box will always show when logging with a prefix.",
                    "default": false
                },
                "wrap-console-log.alwaysUsePrefix": {
                    "type": "boolean",
                    "title": "Always log with the wrapped word as prefix.",
                    "description": "If this is set to 'true' even the \"non-prefix-commands\" will log with prefix.",
                    "default": false
                },
                "wrap-console-log.autoFormat": {
                    "type": "boolean",
                    "title": "[Experimental] Format inserted lines",
                    "description": "When new lines has been created by wrapping Up or Down the extension will run \"editor.action.formatSelection\" on a selection with the new line and its line above and below.",
                    "default": false
                },
                "wrap-console-log.formatDocument": {
                    "type": "boolean",
                    "title": "Format the document after every wrap",
                    "description": "If this is set to 'true' the extension will run \"editor.action.formatDocument\" after every wrap.",
                    "default": false
                }
            }
        },
        "commands": [
            {
                "command": "midnight.console.log.wrap.Inline",
                "title": "Wrap 'console.log' [Inline]"
            },
            {
                "command": "midnight.console.log.wrap.Up",
                "title": "Wrap 'console.log' [Up]"
            },
            {
                "command": "midnight.console.log.wrap.Down",
                "title": "Wrap 'console.log' [Down]"
            },
            {
                "command": "midnight.console.log.wrap.InlinePrefix",
                "title": "Wrap 'console.log' [Inline] [Prefix]"
            },
            {
                "command": "midnight.console.log.wrap.UpPrefix",
                "title": "Wrap 'console.log' [Up] [Prefix]"
            },
            {
                "command": "midnight.console.log.wrap.DownPrefix",
                "title": "Wrap 'console.log' [Down] [Prefix]"
            },
            {
                "command": "midnight.console.log.wrap.InlinePrefixInput",
                "title": "Wrap 'console.log' [Inline] [Prefix-Input]"
            },
            {
                "command": "midnight.console.log.wrap.UpPrefixInput",
                "title": "Wrap 'console.log' [Up] [Prefix-Input]"
            },
            {
                "command": "midnight.console.log.wrap.DownPrefixInput",
                "title": "Wrap 'console.log' [Down] [Prefix-Input]"
            }
        ],
        "keybindings": [
            {
                "command": "midnight.console.log.wrap.Inline",
                "key": "ctrl+alt+w w",
                "when": "editorTextFocus"
            },
            {
                "command": "midnight.console.log.wrap.Down",
                "key": "ctrl+alt+w down",
                "when": "editorTextFocus"
            },
            {
                "command": "midnight.console.log.wrap.Up",
                "key": "ctrl+alt+w up",
                "when": "editorTextFocus"
            },
            {
                "command": "midnight.console.log.wrap.InlinePrefix",
                "key": "ctrl+alt+w ctrl+alt+w",
                "when": "editorTextFocus"
            },
            {
                "command": "midnight.console.log.wrap.DownPrefix",
                "key": "ctrl+alt+w ctrl+alt+down",
                "when": "editorTextFocus"
            },
            {
                "command": "midnight.console.log.wrap.UpPrefix",
                "key": "ctrl+alt+w ctrl+alt+up",
                "when": "editorTextFocus"
            },
            {
                "command": "midnight.console.log.wrap.InlinePrefixInput",
                "key": "ctrl+shift+alt+w ctrl+shift+alt+w",
                "when": "editorTextFocus"
            },
            {
                "command": "midnight.console.log.wrap.DownPrefixInput",
                "key": "ctrl+shift+alt+w ctrl+shift+alt+down",
                "when": "editorTextFocus"
            },
            {
                "command": "midnight.console.log.wrap.UpPrefixInput",
                "key": "ctrl+shift+alt+w ctrl+shift+alt+up",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}