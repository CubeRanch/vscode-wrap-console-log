{
    "name": "vscode-wrap-console-log",
    "displayName": "Wrap Console Log",
    "description": "Auto select and wrap console.log",
    "version": "1.0.0",
    "publisher": "midnightsyntax",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onCommand:midnight.console.log.Wrap.Inline",
        "onCommand:midnight.console.log.Wrap.Up",
        "onCommand:midnight.console.log.Wrap.Down"
    ],
    "main": "out/extension.js",
    "contributes": {
        "commands": [{
            "command": "midnight.console.log.Wrap.Inline",
            "title": "Wrap 'console.log' [Inline]"
        },{
            "command": "midnight.console.log.Wrap.Up",
            "title": "Wrap 'console.log' [Up]"
        },
        {
            "command": "midnight.console.log.Wrap.Down",
            "title": "Wrap 'console.log' [Down]"
        }],
        "keybindings": [
            {
                "command": "midnight.console.log.Wrap.Inline",
                "key": "ctrl+alt+w w"
            },
            {
                "command": "midnight.console.log.Wrap.Down",
                "key": "ctrl+alt+w down"
            },
            {
                "command": "midnight.console.log.Wrap.Up",
                "key": "ctrl+alt+w up"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}