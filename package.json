{
    "name": "vscode-wrap-console-log",
    "displayName": "Wrap Console Log",
    "description": "Wrap to console.log by word or selection",
    "repository": {
        "type": "git",
        "url": "https://github.com/midnightsyntax/vscode-wrap-console-log"
    },
    "version": "1.2.0",
    "publisher": "midnightsyntax",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Formatters"
    ],
    "keywords": [
        "wrap",
        "console",
        "log"
    ],
    "activationEvents": [
        "onCommand:midnight.console.log.wrap.Inline",
        "onCommand:midnight.console.log.wrap.InlineArg",
        "onCommand:midnight.console.log.wrap.Up",
        "onCommand:midnight.console.log.wrap.UpArg",
        "onCommand:midnight.console.log.wrap.Down",
        "onCommand:midnight.console.log.wrap.DownArg"
    ],
    "main": "out/extension.js",
    "contributes": {
        "configuration": {
            "title": "Wrap Console Log",
            "type": "object",
            "properties": {
                "wrap-console-log.customPrefix": {
                    "type": "boolean",
                    "description": "Display an inputbox to specify custom prefix.",
                    "default": false
                },
                "wrap-console-log.autoFormat": {
                    "type": "boolean",
                    "description": "Automatically try to format inserted lines.",
                    "default": false
                },
                "wrap-console-log.formatDocument": {
                    "type": "boolean",
                    "description": "Run \"editor.action.formatDocument\" after every Wrap.",
                    "default": false
                }
            }
        },
        "commands": [
            {
                "command": "midnight.console.log.wrap.Inline",
                "title": "Wrap 'console.log' [Inline]"
            },
            {
                "command": "midnight.console.log.wrap.Up",
                "title": "Wrap 'console.log' [Up]"
            },
            {
                "command": "midnight.console.log.wrap.Down",
                "title": "Wrap 'console.log' [Down]"
            },
            {
                "command": "midnight.console.log.wrap.InlineArg",
                "title": "Wrap 'console.log' [Inline] with argument"
            },
            {
                "command": "midnight.console.log.wrap.UpArg",
                "title": "Wrap 'console.log' [Up] with argument"
            },
            {
                "command": "midnight.console.log.wrap.DownArg",
                "title": "Wrap 'console.log' [Down] with argument"
            }
        ],
        "keybindings": [
            {
                "command": "midnight.console.log.wrap.Inline",
                "key": "ctrl+alt+w w"
            },
            {
                "command": "midnight.console.log.wrap.Down",
                "key": "ctrl+alt+w down"
            },
            {
                "command": "midnight.console.log.wrap.Up",
                "key": "ctrl+alt+w up"
            },
            {
                "command": "midnight.console.log.wrap.InlineArg",
                "key": "ctrl+alt+w ctrl+alt+w"
            },
            {
                "command": "midnight.console.log.wrap.DownArg",
                "key": "ctrl+alt+w ctrl+alt+down"
            },
            {
                "command": "midnight.console.log.wrap.UpArg",
                "key": "ctrl+alt+w ctrl+alt+up"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}